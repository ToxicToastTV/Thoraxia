    "gateway": {
      "root": "apps/gateway",
      "sourceRoot": "apps/gateway/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/gateway",
